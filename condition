int main() {

  int n;
  cout<< "enter the the value of n"<< endl;
  cin >> n;
  if (n > 0) {
    cout << "n is positive" << endl;
  } else {
    cout << "n is negative" << endl;
  }
}

int main (){
  int a,b;
  cout<<"enter vale of a:"<<endl;
  cin>>a;

  cout<<"enter the value of b: "<<endl;
  cin>>b;

  if(a>b){
    cout<<"a is greater then b "<<endl;
  }
  else{
    cout<<"b is greater then a"<<endl;
  }
}

